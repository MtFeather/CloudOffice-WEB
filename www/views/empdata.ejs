<% include header.ejs %>
<div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th>編號</th>
      <th>姓名</th>
      <th>帳號</th>
      <th>密碼</th>
      <th>性別</th>
      <th>電子郵件</th>
      <th>部門</th>
      <th>階級</th>
      <th>註冊時間</th>
    </tr>
  </thead>
  <tbody>
    <% items.forEach(function(user) {%>
    <tr>
	<td><%= user.eid %></td>
        <td><%= user.name %></td>
        <td><%= user.account %></td>
        <td><%= user.passwd %></td>
        <td><%= user.sex %></td>
        <td><%= user.email %></td>
        <td><%= user.dep %></td>
        <td><%= user.level %></td>
        <td><%= user.join_time %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
</div>
<% include footer.ejs %>
